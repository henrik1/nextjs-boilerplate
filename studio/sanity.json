{
  "root": true,
  "project": {
    "name": "<PROJECT NAME>"
  },
  "api": {
    "projectId": "<YOUR PROJECT ID>",
    "dataset": "<YOUR DATASET NAME, eg. production | development>"
  },
  "plugins": [
    "@sanity/base",
    "@sanity/components",
    "@sanity/default-layout",
    "@sanity/default-login",
    "@sanity/desk-tool",
    "autocomplete-tags",
    "@sanity/code-input"
  ],
  "env": {
    "development": {
      "plugins": [
        "@sanity/vision"
      ]
    }
  },
  "parts": [
    {
      "name": "part:@sanity/base/schema",
      "path": "./schemas/schema"
    }
  ]
}
